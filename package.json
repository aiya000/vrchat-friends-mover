{
  "name": "vrchat-friends-mover",
  "version": "0.1.0",
  "description": "This is a way to send friends requests to all friends of an another VRChat account's friends :tada:",
  "main": "src/main.ts",
  "scripts": {
    "start": "yarn build && node ./dist/main.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "lint": "yarn prettier && yarn eslint",
    "eslint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "prettier": "prettier --check 'src/**/*.{ts,js,vue}' --write",
    "test": "yarn testify && jest",
    "testify": "tsdoc-testify --fileMatch 'src/**/*.ts'",
    "test:watch": "watchexec --exts vue,ts --ignore '**/*.doctest.ts' --restart -- yarn test",
    "test:clean": "rm **/*.doctest.ts",
    "repl": "ts-node -r tsconfig-paths/register"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "io-ts": "^2.2.16",
    "jest": "^26.6.3",
    "marron-glace": "^0.0.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsdoc-testify": "^0.0.3",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiya000/vrchat-friends-mover.git"
  },
  "keywords": [
    "VRChat"
  ],
  "author": "aiya000",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aiya000/vrchat-friends-mover/issues"
  },
  "homepage": "https://github.com/aiya000/vrchat-friends-mover"
}
